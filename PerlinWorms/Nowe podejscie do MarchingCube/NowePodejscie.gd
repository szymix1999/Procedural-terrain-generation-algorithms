var surface : SurfaceTool

var voxels = []
var size : Vector3

func _init(size, voxels):
	surface = SurfaceTool.new()
	surface.begin(Mesh.PRIMITIVE_TRIANGLES)
	
	self.voxels = voxels
	self.size = size
	for x in range(0, size.x):
		for y in range(0, size.y):
			for z in range(0,size.z):
				process_cube(Vector3(x,y,z))
				
	surface.generate_normals()
	surface.set_material(preload("res://Materials/terrain.material"))
	var mi = MeshInstance.new()
	mi.mesh = surface.commit()
	add_child(mi)

func getValueAtPoint(id: Vector3):
	return voxels[id.x][id.y][id.z]
	
func calculateVertexPosition(coordA: Vector3, coordB: Vector3):
	var valueA = getValueAtPoint(coordA)
	var valueB = getValueAtPoint(coordB)
	
	var t = (0 - valueA) / (valueB-valueA)
	return coordA + t * (coordB - coordA)

func process_cube(v : Vector3):
	if voxels != null:
		if v.x >= size.x-1 or v.y >= size.y-1 or v.z >= size.z-1: return
	
	var cornerCoords=[
		v + Vector3(0,0,0),
		v + Vector3(1,0,0),
		v + Vector3(1,0,1),
		v + Vector3(0,0,1),
		v + Vector3(0,1,0),
		v + Vector3(1,1,0),
		v + Vector3(1,1,1),
		v + Vector3(0,1,1)
	]
	
	var cubeConfiguration = 0
	for i in range(0,8,1):
		if getValueAtPoint(cornerCoords[i]) < 0:
			cubeConfiguration |= (1<<i)
	#print("CONFIGURATION VOXEL: ", cubeConfiguration)

	var edgeIndices = triTable[cubeConfiguration]
	
	var cornerIndexAFromEdge = [0,1,2,3,4,5,6,7,0,1,2,3]
	var cornerIndexBFromEdge = [1,2,3,0,5,6,7,4,4,5,6,7]
	for i in range(0,16,3):
		if edgeIndices[i] == -1: break
		
		var a0 = cornerIndexAFromEdge[edgeIndices[i+0]]
		var a1 = cornerIndexBFromEdge[edgeIndices[i+0]]
		
		var b0 = cornerIndexAFromEdge[edgeIndices[i+1]]
		var b1 = cornerIndexBFromEdge[edgeIndices[i+1]]
		
		var c0 = cornerIndexAFromEdge[edgeIndices[i+2]]
		var c1 = cornerIndexBFromEdge[edgeIndices[i+2]]
		
		
		var vertices :PoolVector3Array
		
		vertices.append(calculateVertexPosition(cornerCoords[a0], cornerCoords[a1]))
		vertices.append(calculateVertexPosition(cornerCoords[b0], cornerCoords[b1]))
		vertices.append(calculateVertexPosition(cornerCoords[c0], cornerCoords[c1]))
		
		surface.add_triangle_fan(vertices)
